<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.eodeatzy.empMapper">
	
	<select id="userInfo" parameterType="java.lang.String" resultType="java.lang.Integer">
		select count(*) 
		FROM
		tb_user_p
		where u_p_id =#{u_p_id}
	</select>


</mapper>